{
  // Launch configuration for debugging `eval_goemotions.py` from VS Code Run/Debug (bug button).
  // This configuration runs the file with a small default sample size so it finishes quickly
  // and avoids triggering long trainings by default. You can edit `args` as needed.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run eval_goemotions (quick)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/tests/evaluation/eval_goemotions.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--n-samples", "10",
        "--llm-provider", "openai",
        "--output-dir", "outputs/goemotions_experiments/debug_run",
        "--no-baselines"
      ],
      "env": {
        // Set any environment variables you need here, e.g. OPENAI_API_KEY
        // "OPENAI_API_KEY": "your_key_here"
      }
    },
    {
      "name": "Python: Run eval_goemotions (full files)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/tests/evaluation/eval_goemotions.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": [
        "--full",
        "--llm-provider", "openai",
        "--output-dir", "outputs/goemotions_experiments/full_run"
      ],
      "env": {}
    }
  ]
}
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run eval_goemotions.py (No Debug)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tests/evaluation/eval_goemotions.py",
      "console": "integratedTerminal",
      "args": ["--n-samples", "20"],
      "env": {"PYTHONPATH": "${workspaceFolder}"},
      "justMyCode": false
    },
    {
      "name": "Debug eval_goemotions.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tests/evaluation/eval_goemotions.py",
      "console": "integratedTerminal",
      "args": ["--n-samples", "20"],
      "env": {"PYTHONPATH": "${workspaceFolder}"},
      "justMyCode": false
    },
    {
      "name": "Run eval_goemotions.py (With Inputs)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tests/evaluation/eval_goemotions.py",
      "console": "integratedTerminal",
      "args": ["--n-samples", "${input:nSamples}", "--llm-provider", "${input:llmProvider}", "--cache-dir", "${input:cacheDir}"],
      "env": {"PYTHONPATH": "${workspaceFolder}"},
      "justMyCode": false
    }
  ],
  "inputs": [
    {
      "id": "nSamples",
      "type": "promptString",
      "description": "Number of samples for train/val/test (or leave blank to use percentages)",
      "default": "20"
    },
    {
      "id": "llmProvider",
      "type": "pickString",
      "description": "LLM provider",
      "options": ["openai", "deepseek"],
      "default": "openai"
    },
    {
      "id": "cacheDir",
      "type": "promptString",
      "description": "Cache directory",
      "default": "cache"
    }
  ]
}
